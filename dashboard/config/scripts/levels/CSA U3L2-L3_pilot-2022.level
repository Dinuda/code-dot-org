    "start_sources": {
      "MyConsole.java": {
        "text": "public class MyConsole {\n  public static void main(String[] args) {\n\n    String[] countries = {\"Australia\", \"Canada\", \"China\", \"France\", \"Haiti\", \"India\",\n                         \"Jamaica\", \"Mexico\", \"United Kingdom\", \"United States\"};\n    \n    double[] internetUsage = {86.55, 91, 54.3, 82.04, 32.47, 34.45, 55.07, 65.77, 94.9, 87.27};\n\n    System.out.println(\"Countries: \" + countries);\n    System.out.println(\"Percent Using the Internet: \" + internetUsage);\n\n    int index = 0;\n\n    while (index <= countries.length) {\n      System.out.println(countries[index] + \": \" + internetUsage[index] + \"% using the Internet\");\n      index += 1;\n    }\n    \n  }\n}",
        "isVisible": true
